= simple_form_for @material_por_pedido, url: materiales_por_pedido_path, method: :put do |f|
    = f.input :id , label: 'ID:'
    = f.input :id_sistema , label: 'Num de sistema:'
    = f.input :sistema , label: 'Sistema:'
    = f.input :id_material , label: 'Num de material:'
    = f.input :id_pedido , label: 'Num de pedido:'
    = f.input :cantidad , label: 'Cantidad:'
    = f.button :submit, class: 'btn btn-primary'
